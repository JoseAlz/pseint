Proceso SUMA_N	
	// Desarrole un algoritmo en el que se muestre la suma de 4 valores ingresados por teclado.
	Definir valor1,valor2,valor3,valor4 como Entero;
	
	Imprimir "Ingrese 4 valores enteros:";
	Leer valor1,valor2,valor3,valor4;
	
	Imprimir "La suma de los 4 valores ingresados es ", valor1 + valor2 + valor3 + valor4;
FinProceso
